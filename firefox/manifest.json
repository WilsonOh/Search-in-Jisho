{
  "name": "Search in Jisho",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "menu.js"
    ]
  },
  "icons": {
    "16": "128.png"
  }
}
